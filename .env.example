# Example .env for production. NEVER commit real secrets to git.
DATABASE_URL=postgresql://DB_USER:DB_PASS@DB_HOST:5432/DB_NAME?schema=public
NODE_ENV=production
PORT=3000
JWT_SECRET=replace_with_secure_jwt_secret
COOKIE_SECRET=replace_with_cookie_secret
GOOGLE_APPLICATION_CREDENTIALS=/app/gcs-key.json
GCS_BUCKET=your-gcs-bucket-name
GCS_PROJECT=your-gcs-project-id
# Optional: set if you decode base64 key into file at startup
GCS_KEY_B64=
# Prisma
SHADOW_DATABASE_URL=
# Other optional config
SENTRY_DSN=

# For local debugging only
# LOG_LEVEL=info